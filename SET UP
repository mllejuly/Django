#创建项目
C:\Users\mlle.july>
mkdir project

#创建虚拟环境
C:\Users\mlle.july\project>
python -m virtualenv venv

#激活虚拟环境
C:\Users\mlle.july\swift\venv\Scripts>
activate

#安装Django
(venv) C:\Users\mlle.july\project\venv>
pip install Django==1.10.7
pip install pyodbc==4.0.17

#创建项目
(venv) C:\Users\mlle.july\project\venv>
django-admin startproject project .

(venv) C:\Users\mlle.july\project\venv>
python manage.py migrate

#创建应用程序
(venv) C:\Users\mlle.july\project\venv>
python manage.py startapp app

#add app to project/settings.py

(venv) C:\Users\mlle.july\project\venv>
python manage.py makemigrations app

(venv) C:\Users\mlle.july\project\venv>
python manage.py migrate app


#创建模型
#app/models.py
from __future__ import unicode_literals
from django.db import models


class AppModel(models.Model):
    text = models.TextField()


#创建视图
#app/views.py
from django.shortcuts import render


def appHome(request):
    return render(request, 'app-home.html')


#创建URL
#project/urls.py
from django.conf.urls import include, url
from django.contrib import admin

urlpatterns = [
    url(r'^admin/', include(admin.site.urls)),
    url(r'', include('app.urls')),
]

#app/urls.py
from django.conf.urls import url
import views

urlpatterns = [
    url(r'^app/$', views.appHome, name='appHome'),
]




